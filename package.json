{
  "name": "eslint-config-intolerable-style-guide",
  "version": "0.0.0-development",
  "description": "The ultimate oppressive style guide.",
  "scripts": {
    "build": "rm -rf dist && tsc -p ./tsconfig.build.json",
    "lint": "tsc -p ./tsconfig.build.json && eslint src/**",
    "test": "tsc -p ./tsconfig.build.json && npm run --prefix=example-project lint",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "author": "Team Fraxes",
  "repository": {
    "type": "git",
    "url": "git://github.com/solarflare045/intolerable-style-guide.git"
  },
  "engines": {
    "node": ">=18.18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "ISC",
  "files": [
    "*.md",
    "dist/**"
  ],
  "dependencies": {
    "@eslint/js": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.2.5",
    "semver": "^7.5.2",
    "typescript-eslint": "^7.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/semver": "^7.5.8",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "husky": "^7.0.2",
    "semantic-release": "^23.0.6",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "typescript": "*"
  }
}
